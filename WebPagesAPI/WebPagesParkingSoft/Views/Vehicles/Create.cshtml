@model API_AppParkingSoft.DAL.Entities.Vehicle

@{
    ViewData["Title"] = "Create";
}

<h1>Crear Vehiculo</h1>

<h4>Vehiculo</h4>
<hr />

a<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="../Shared/PartialView/_VehiclesCreateEdit.cshtml">

            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-outline-primary" />
                <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
